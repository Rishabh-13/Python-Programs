    "from difflib import SequenceMatcher\n",
    "\n",
    "text1 = input(\"Enter 1st sentence : \")\n",
    "text2 = input(\"Enter 2nd sentence : \")\n",
    "sequenceScore = SequenceMatcher(None, text1, text2).ratio()\n",
    "\n",
    "print(f\"Both are {sequenceScore * 100} % similar\") "
